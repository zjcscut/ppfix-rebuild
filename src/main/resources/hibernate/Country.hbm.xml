<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ppfix.web.model.Country" table="country">
        <id name="Id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="countryname" type="java.lang.String">
            <column name="countryname" length="255" />
        </property>

        <property name="countrycode" type="java.lang.String">
            <column name="countrycode" length="255"/>
        </property>

    </class>
</hibernate-mapping>